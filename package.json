{
  "name": "breakfast-api",
  "version": "1.0.0",
  "description": "Breakfast API",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build-development": "webpack --mode development",
    "watch": "webpack -w --mode development",
    "test": "ts-mocha -r 'reflect-metadata' src/**/*.spec.ts",
    "integration-test": "ts-mocha -r 'reflect-metadata' --timeout 30000 --slow 15000 --async-only e2e/**/*.spec.ts",
    "analyze": "webpack --mode production --env.analyze",
    "lint": "eslint --ext .js,.ts .",
    "start-api": "sam local start-api --skip-pull-image --docker-network breakfast",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "author": "Alex Haslehurst",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.39",
    "@types/chai": "^4.2.0",
    "@types/faker": "^4.1.9",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.7.4",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "aws-sam-webpack-plugin": "^0.5.1",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "mocha": "^7.0.1",
    "mocha-typescript": "^1.1.17",
    "ts-loader": "^6.2.1",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.3.0",
    "typemoq": "^2.1.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "aws-sdk": "^2.604.0",
    "class-transformer": "^0.2.3",
    "inversify": "^5.0.1",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.16",
    "typeorm": "^0.2.22",
    "uuid": "^3.4.0",
    "winston": "^3.2.1"
  }
}
